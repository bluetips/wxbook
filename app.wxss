.page {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #F3F8FF;
    box-sizing: border-box;
    overflow: hidden;
}

.page .content {
    position: relative;
    -webkit-box-flex: 1;
    flex: 1;
    height: 100%;
    overflow: hidden;
}

.page .content .inner,.page .content .scroll-view_V {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.page .list-group {
    background: #ffffff;
}

.page .list-group .list-item {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    padding: 29rpx 30rpx;
    background: #FFFFFF;
    border: 0 none;
    line-height: 1;
}

.page .list-group .list-item .ico {
    width: 72rpx;
    height: 72rpx;
    background-size: cover;
}

.page .list-group .list-item .ico+.title {
    -webkit-box-flex: 1;
    flex: 1;
    margin-left: 20rpx;
}

.page .list-group .list-item .iconfont {
    font-size: 32rpx;
}

.page .list-group .list-item .iconfont+.title {
    -webkit-box-flex: 1;
    flex: 1;
    margin-left: 20rpx;
}

.page .list-group .list-item .title {
    font-size: 32rpx;
    text-align: left;
}

.page .list-group .list-item .title.bold {
    font-weight: bold;
}

.page .list-group .list-item .after {
    font-size: 28rpx;
    color: #999999;
    -webkit-box-flex: 1;
    flex: 1;
    flex-shrink: 0;
    text-align: right;
    margin-left: 20rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.page .list-group .list-item .icon-tuceng {
    color: #999999;
    margin-left: 20rpx;
}

.page .list-group .list-item:after {
    border: 0 none;
}

.page .list-group .list-item:not(:last-child):after {
    position: absolute;
    bottom: 0;
    left: 30rpx;
    right: 30rpx;
    content: '';
    height: 1px;
    background: #D8D8D8;
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.page .whitespace {
    height: 20rpx;
}

.page .finish {
    padding: 40rpx 0;
}

.page .empty {
    text-align: center;
    font-size: 28rpx;
    color: #999999;
}

.page .empty+.button-primary {
    display: block;
    height: 75rpx;
    line-height: 75rpx;
    text-align: center;
    border: 0 none;
    background: none;
    font-size: 32rpx;
    color: #ffffff;
    background: #21D3AB;
    border-radius: 37.5rpx;
    height: 80rpx;
    line-height: 80rpx;
    margin-top: 60rpx;
}

.page .empty+.button-primary:after {
    border: 0 none;
}

.page .button-fixed-share {
    position: fixed;
    width: 150rpx;
    height: 150rpx;
    padding: 0;
    bottom: 10rpx;
    right: 10rpx;
    background: transparent;
}

.page .button-fixed-share.animated {
    animation-iteration-count: infinite;
}

.page .button-fixed-share:after {
    border: 0 none;
}

.page .button-fixed-share .share {
    width: 150rpx;
    height: 150rpx;
}

.animated {
    animation-duration: 1s;
    animation-delay: .5s;
    animation-fill-mode: both;
}

.tada {
    animation-name: tada;
}

@-webkit-keyframes tada {
    0% {
        transform: scaleX(1);
    }

    10%,20% {
        transform: scale3d(0.9,0.9,0.9) rotate(-3deg);
    }

    30%,50%,70%,90% {
        transform: scale3d(1.01,1.01,1.01) rotate(3deg);
    }

    40%,60%,80% {
        transform: scale3d(1.01,1.01,1.01) rotate(-3deg);
    }

    to {
        transform: scaleX(1);
    }
}

@keyframes tada {
    0% {
        transform: scaleX(1);
    }

    10%,20% {
        transform: scale3d(0.9,0.9,0.9) rotate(-3deg);
    }

    30%,50%,70%,90% {
        transform: scale3d(1.01,1.01,1.01) rotate(3deg);
    }

    40%,60%,80% {
        transform: scale3d(1.01,1.01,1.01) rotate(-3deg);
    }

    to {
        transform: scaleX(1);
    }
}

.pulse {
    animation-name: pulse;
}

@-webkit-keyframes pulse {
    0% {
        transform: scaleX(1);
    }

    50% {
        transform: scale3d(1.05,1.05,1.05);
    }

    to {
        transform: scaleX(1);
    }
}

@keyframes pulse {
    0% {
        transform: scaleX(1);
    }

    50% {
        transform: scale3d(1.05,1.05,1.05);
    }

    to {
        transform: scaleX(1);
    }
}
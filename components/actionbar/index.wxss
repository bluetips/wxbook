.bar.day {
    background: #ffffff;
    color: #333333;
}

.bar.day .button-plain {
    display: block;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    border: 0 none;
    background: none;
    font-size: 28rpx;
    color: #666666;
    color: #333333;
}

.bar.day .button-plain:after {
    border: 0 none;
}

.bar.day .button-plain.button-disable {
    color: #999999;
}

.bar.day .button-plain.active {
    color: #21D3AB;
}

.bar.night {
    background: #333333;
    color: #666666;
}

.bar.night .button-plain {
    display: block;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    border: 0 none;
    background: none;
    font-size: 28rpx;
    color: #666666;
    color: #999999;
}

.bar.night .button-plain:after {
    border: 0 none;
}

.bar.night .button-plain.button-disable {
    color: #666666;
}

.bar.night .button-plain.active {
    color: #21D3AB;
}

.bar .top {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    height: 100rpx;
    line-height: 100rpx;
    box-sizing: border-box;
    padding: 0 20rpx;
}

.bar .top:after {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    content: '';
    height: 1px;
    background: #D8D8D8;
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.bar .top .action {
    -webkit-box-flex: 1;
    flex: 1;
}

.bar .top .action .button-plain {
    padding: 0;
}

.bar .top .action:first-child .button-plain {
    text-align: left;
    padding-left: 20rpx;
}

.bar .top .action:last-child .button-plain {
    text-align: right;
    padding-right: 20rpx;
}

.bar .top .title {
    text-align: center;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    width: 420rpx;
    font-size: 28rpx;
}

.bar .top .label {
    font-size: 32rpx;
    padding-left: 30rpx;
}

.bar .top .setting {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: end;
    justify-content: flex-end;
    -webkit-box-align: center;
    align-items: center;
}

.bar .top .setting .button-plain {
    height: 100rpx;
    line-height: 100rpx;
    text-align: right;
    padding: 0 30rpx 0 40rpx;
}

.bar .top .setting .button-plain:first-child {
    padding: 0 30rpx 0 40rpx;
}

.bar .top .setting .button-plain:last-child {
    padding: 0 40rpx 0 30rpx;
}

.bar .top .setting .button-plain .iconfont {
    font-size: 32rpx;
}

.bar .top .setting .size {
    font-size: 36rpx;
    width: 60rpx;
    text-align: center;
}

.bar .action-group {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    height: 160rpx;
    box-sizing: border-box;
    padding: 20rpx;
}

.bar .action-group .action {
    -webkit-box-flex: 1;
    flex: 1;
}

.bar .action-group .action+.action {
    margin-left: 30rpx;
}

.bar .action-group .action .button-plain {
    height: 120rpx;
    font-size: 24rpx;
    box-sizing: border-box;
    padding: 16rpx 0 0;
    line-height: 1;
}

.bar .action-group .action .button-plain .inner {
    display: inline-block;
    text-align: center;
}

.bar .action-group .action .button-plain .inner .iconfont {
    font-size: 50rpx;
    margin-bottom: 16rpx;
}

.bar .action-group .action:first-child .button-plain {
    padding-left: 20rpx;
    text-align: left;
}

.bar .action-group .action:last-child .button-plain {
    text-align: right;
    padding-right: 20rpx;
}

.bar .action-group .action:nth-child(4n+2) .button-plain {
    text-align: left;
    padding-left: 40rpx;
}

.bar .action-group .action:nth-child(4n+3) .button-plain {
    text-align: right;
    padding-right: 40rpx;
}
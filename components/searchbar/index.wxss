.searchbar {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    background: #FFFFFF;
    padding: 30rpx;
}

.searchbar .inner {
    -webkit-box-flex: 1;
    flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: relative;
    border-radius: 6rpx;
    height: 60rpx;
    padding: 20rpx;
    background: #F2F2F2;
    -webkit-box-align: center;
    align-items: center;
    box-sizing: border-box;
}

.searchbar .inner .icon-search {
    color: #999999;
    margin-right: 20rpx;
}

.searchbar .inner .input {
    color: #333333;
    font-size: 28rpx;
    width: 100%;
}

.searchbar .inner .input-placeholder {
    color: #999999;
}

.searchbar .button-search {
    display: block;
    height: 75rpx;
    line-height: 75rpx;
    text-align: center;
    border: 0 none;
    background: none;
    font-size: 32rpx;
    color: #333333;
    font-weight: bold;
}

.searchbar .button-search:after {
    border: 0 none;
}